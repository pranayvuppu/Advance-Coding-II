#include <stdio.h>
#include <string.h>

void findOnesComplement(char binary[]) {
    int i;
    printf("One's complement: ");
    for (i = 0; binary[i] != '\0'; i++) {
        if (binary[i] == '0') {
            printf("1");
        } else if (binary[i] == '1') {
            printf("0");
        } else {
            printf("\nInvalid binary number.\n");
            return;
        }
    }
    printf("\n");
}

int main() {
    char binary[100];

    printf("Enter a binary number: ");
    scanf("%s", binary);

    findOnesComplement(binary);

    return 0;
}
